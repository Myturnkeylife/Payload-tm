{
  "name": "payload-example-auth",
  "description": "Payload authentication example.",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts && pnpm seed && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts NODE_OPTIONS=--no-deprecation next build",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "seed": "npm run payload migrate:fresh"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.0.0-beta.14",
    "@payloadcms/next": "3.0.0-beta.14",
    "@payloadcms/richtext-slate": "3.0.0-beta.14",
    "@payloadcms/ui": "3.0.0-beta.14",
    "cross-env": "^7.0.3",
    "next": "14.3.0-canary.7",
    "payload": "3.0.0-beta.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.1.6",
    "@payloadcms/eslint-config": "^1.1.1",
    "@swc/core": "^1.4.14",
    "@swc/types": "^0.1.6",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "tsx": "^4.7.1",
    "typescript": "5.4.4"
  }
}
