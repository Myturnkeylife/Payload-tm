{
  "name": "@payloadcms/next",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.d.ts",
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "exports": {
    "./utilities/*": {
      "import": "./src/utilities/*.ts",
      "require": "./src/utilities/*.ts",
      "types": "./src/utilities/*.ts"
    },
    "./layouts/*": {
      "import": "./src/layouts/*.ts",
      "require": "./src/layouts/*.ts",
      "types": "./src/layouts/*.ts"
    },
    "./pages/*": {
      "import": "./src/pages/*.ts",
      "require": "./src/pages/*.ts",
      "types": "./src/pages/*.ts"
    },
    "./routes": {
      "import": "./src/routes/index.ts",
      "require": "./src/routes/index.ts"
    },
    "./graphql": {
      "import": "./src/graphql/routes/handler.ts",
      "require": "./src/graphql/routes/handler.ts"
    },
    "./graphql-playground": {
      "import": "./src/graphql/routes/playground.ts",
      "require": "./src/graphql/routes/playground.ts"
    }
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "workspace:*",
    "payload": "workspace:*",
    "sass": "^1.70.0",
    "ts-essentials": "7.0.3"
  },
  "dependencies": {
    "@faceless-ui/modal": "2.0.1",
    "@payloadcms/translations": "workspace:^",
    "@payloadcms/ui": "workspace:*",
    "@types/busboy": "^1.5.3",
    "busboy": "^1.6.0",
    "deep-equal": "2.2.2",
    "graphql-playground-html": "1.6.30",
    "graphql-query-complexity": "0.12.0",
    "graphql-scalars": "1.22.2",
    "graphql-type-json": "0.3.2",
    "path-to-regexp": "^6.2.1",
    "pluralize": "8.0.0",
    "react-diff-viewer-continued": "3.2.6",
    "react-toastify": "8.2.0"
  },
  "peerDependencies": {
    "http-status": "1.6.2",
    "i18next": "22.5.1",
    "next": "^14.0.0",
    "payload": "^2.0.0"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      "./utilities/*": {
        "import": "./dist/utilities/*.js",
        "require": "./dist/utilities/*.js",
        "types": "./dist/utilities/*.d.ts"
      },
      "./layouts/*": {
        "import": "./dist/layouts/*.js",
        "require": "./dist/layouts/*.js",
        "types": "./dist/layouts/*.d.ts"
      },
      "./pages/*": {
        "import": "./dist/pages/*.js",
        "require": "./dist/pages/*.js",
        "types": "./dist/pages/*.d.ts"
      },
      "./app/*": {
        "import": "./dist/app/*.js",
        "require": "./dist/app/*.js",
        "types": "./dist/app/*.d.ts"
      },
      "./routes": {
        "import": "./dist/routes/index.js",
        "require": "./dist/routes/index.js",
        "types": "./dist/routes/index.d.ts"
      },
      "./graphql": {
        "import": "./dist/graphql/routes/handler.js",
        "require": "./dist/graphql/routes/handler.js",
        "types": "./dist/graphql/routes/handler.ts"
      },
      "./graphql-playground": {
        "import": "./dist/graphql/routes/playground.js",
        "require": "./dist/graphql/routes/playground.js",
        "types": "./dist/graphql/routes/playground.ts"
      }
    },
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ]
}
