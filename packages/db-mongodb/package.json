{
  "name": "@payloadcms/db-mongodb",
  "version": "0.0.1",
  "description": "The officially supported MongoDB database adapter for Payload",
  "main": "./src/index.ts",
  "type": "module",
  "types": "./src/index.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "main": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts",
    "module": "./dist/esm/index.js",
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      }
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "repository": "https://github.com/payloadcms/payload",
  "author": "Payload CMS, Inc.",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:tsc": "tsc",
    "build": "pnpm build:swc:esm && pnpm build:swc:cjs && node ./scripts/build.js",
    "build:withts": "pnpm build:swc:esm && pnpm build:swc:cjs && pnpm build:tsc:esm && pnpm build:tsc:cjs",
    "build:swc:esm": "swc ./src -d ./dist/esm --config-file .swcrc-esm",
    "build:swc:cjs": "swc ./src -d ./dist/cjs --config-file .swcrc-cjs",
    "build:tsc:esm": "tsc --module NodeNext --target ESNext --emitDeclarationOnly --outDir dist/esm",
    "build:tsc:cjs": "tsc --module NodeNext --target ESNext --emitDeclarationOnly --outDir dist/cjs"
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "workspace:*",
    "@swc/cli": "^0.1.62",
    "@types/mongoose-aggregate-paginate-v2": "1.0.9",
    "mongodb-memory-server": "8.13.0",
    "payload": "workspace:*"
  },
  "dependencies": {
    "bson-objectid": "2.0.4",
    "deepmerge": "4.3.1",
    "get-port": "5.1.1",
    "mongoose": "6.11.4",
    "mongoose-aggregate-paginate-v2": "1.0.6",
    "mongoose-paginate-v2": "1.7.22",
    "uuid": "9.0.0"
  }
}
